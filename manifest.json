{
   "background": {
      "scripts": [ "scripts/background.js" ]
   },
   "browser_action": {
      "default_title": ""
   },
   "content_scripts": [ {
      "js": [ "scripts/eventListener.js" ],
      "matches": [ "\u003Call_urls>" ],
      "run_at": "document_start"
   } ],
   "description": "This extension auto scroll down on tabs and switch to the next when it reaches the bottom of the current tab. Â©Copyright CyonTech.ca",
   "icons": {
      "128": "icon128.png",
      "32": "icon32.png"
   },
   "key": "",
   "manifest_version": 2,
   "name": "Automatic Scrolling Tabs",
   "options_page": "options.html",
   "permissions": [ "activeTab", "tabs", "background", "\u003Call_urls>", "*://*/*", "storage" ],
   "short_name": "AutoScrollTabs",
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.1.4"
}
